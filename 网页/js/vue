<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Vue 3 CDN Example</title>
  </head>
  <body>
    <div id="app">
      <p>{{ title }}</p>
      <p>{{ content }}</p>
      <!-- <p>{{1+2+3}}</p>
        <p>{{1>2 / '对': '错'}}</p> -->
      <!-- <p>{{ output() }}</p>
      <p>{{ output() }}</p>
      <p>{{ output() }}</p>
      <p>{{ outputConthet }}</p>
      <p>{{ outputConthet }}</p>
      <p>{{ outputConthet }}</p> -->

      <!-- <p v-text="titleContent">123</p>
      <p v-html="titleContent">123</p> -->

      <!-- <p v-for="(itme,key,index) in obj">这是内容:{{ itme }}{{ key }}{{ index }}</p> -->
      <!-- <p v-if="bool">标签内容</p>
      <p v-show="bool">标签内容</p> -->

      <!-- <p v-bind:title="title">这是内容</p>
      <p :title="title">这是内容</p> -->

      <!-- <button V-on:click="output">按钮</button> -->
      <!-- <button @click="output">按钮</button> -->
      <!-- <button @click="output">按钮</button> -->

      <input type="text" v-model="inputValue" />
      <p v-text="inputValue"></p>

      <input type="text" v-model.trim="inputValue" />
    </div>
    <script src="https://unpkg.com/vue@3/dist/vue.global.prod.js"></script>
    <script>
      const vm = new Vue({
        el: "#app",
        data() {
          return {
            title: "这是标题",
            content: "这是内容",
            htmlContent: "<h2>这是一个h2标签</h2>",
            arr: ["a", "b", "c", "d"],
            obj: { a: 10, b: 20, c: 30 },
            bool: true,
            inputValue: "默认内容",
          };
        },
        methods: {
          output() {
            console.log("method执行了");
            return "标题:" + this.title + "," + " 内容" + this.content;
          },
        },

        methods: {
          outputContent() {
            console.log("computed执行了");
            return "标题:" + this.title + "," + " 内容" + this.content;
          },
        },
        watch: {
          title(newVal, oldVal) {
            console.log("title属性被修改了");
          },
        },
      });
    </script>
  </body>
</html>
